<?xml version="1.0" encoding="UTF-8"?>
<launch>
  <rosparam command="load" file="$(find arena-simulation-setup)/robot/jackal/control.yaml" />
  <node name="controller_spawner" pkg="controller_manager" type="spawner" args="jackal_joint_publisher jackal_velocity_controller">
  </node>
  <node name="odom_pub" pkg="gazebo_utils" type="odom.py"/>
  <node name="velocity_redirect" pkg="gazebo_utils" type="trans.py">
    <param name="cmd_vel_topic" value="/jackal_velocity_controller/cmd_vel" />
  </node>
</launch>
